{
  "name": "sor_demo",
  "version": "1.0.0",
  "main": "index.js",
  "author": "liang xiong <liangxiong@okg.com>",
  "license": "MIT",
  "scripts": {
    "compile-v3-types": "npx typechain --target ethers-v5 --out-dir src/types/v3 './node_modules/@uniswap/?(v3-core|v3-periphery)/artifacts/contracts/**/*.json'",
    "compile-v2-types": "npx typechain --target ethers-v5 --out-dir src/types/v2 './node_modules/@uniswap/?(v2-core|v2-periphery)/build/*UniswapV2*.json'",
    "compile-external-types": "npx typechain --target ethers-v5 --out-dir src/types/other 'src/abis/**/*.json'",
    "build": "run-p compile-v3-types compile-v2-types compile-external-types build:contracts && run-s build:main",
    "build:main": "tsc -p tsconfig.json",
    "build:contracts": "hardhat compile",
    "demo": "hardhat run scripts/demo.ts",
    "start": "ts-node scripts/app.ts",
    "deploy_local": "hardhat run scripts/deploy.ts --network localhost",
    "client_local": "hardhat run scripts/client.ts --network localhost",
    "fix": "yarn prettier && run-s lint:*",
    "prettier": "prettier --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "lint:ts": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore --ext .js,.ts . --fix",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\" --fix",
    "version": "standard-version",
    "prepare-release": "run-s cov:check doc:html version doc:publish",
    "test": "jest",
    "integ-test": "jest test/integ"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@0x/orderbook": "^2.2.7",
    "@0x/types": "^3.3.4",
    "@bitauth/libauth": "^1.17.1",
    "@ethersproject/bignumber": "^5.1.1",
    "@koa/router": "^10.1.1",
    "@oclif/command": "^1.8.0",
    "@oclif/errors": "^1.3.5",
    "@openzeppelin/contracts": "^4.5.0",
    "@uniswap/default-token-list": "^2.0.0",
    "@uniswap/router-sdk": "^1.0.0-beta.7",
    "@uniswap/token-lists": "^1.0.0-beta.25",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v2-sdk": "^3.0.1",
    "@uniswap/v3-periphery": "^1.1.1",
    "@uniswap/v3-sdk": "^3.7.0",
    "async-retry": "^1.3.1",
    "await-timeout": "^1.1.1",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.2",
    "cli-logger": "^0.5.40",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-functional": "^4.0.2",
    "ethereum-types": "^3.5.0",
    "ethers": "^5.1.4",
    "gitbook": "^2.6.9",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "koa": "^2.13.4",
    "lodash": "^4.17.21",
    "mnemonist": "^0.38.3",
    "mongodb": "^4.5.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.2.4",
    "stats-lite": "^2.2.0",
    "surrial": "^2.0.2",
    "tslib": "^1.14.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
